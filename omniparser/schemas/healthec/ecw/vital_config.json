{
  "parser_settings": {
    "version": "omni.2.1",
    "file_format_type": "csv2"
  },
  "file_declaration": {
    "delimiter": "|",
    "replace_double_quotes": true,
    "records": [
      {
        "rows": 1,
        "min": 1,
        "max": 1
      },
      {
        "is_target": true,
        "columns": [
          {
            "name": "apuid",
            "index": 1
          },
          {
            "name": "record_id",
            "index": 2
          },
          {
            "name": "item_id",
            "index": 3
          },
          {
            "name": "patient_id",
            "index": 4
          },
          {
            "name": "patient_account_no",
            "index": 5
          },
          {
            "name": "encounter_id",
            "index": 6
          },
          {
            "name": "provider_npi",
            "index": 7
          },
          {
            "name": "vitals_captured_by",
            "index": 8
          },
          {
            "name": "vitals_name",
            "index": 9
          },
          {
            "name": "standard_vital",
            "index": 10
          },
          {
            "name": "vitals_value",
            "index": 11
          },
          {
            "name": "modified_date",
            "index": 12
          }
        ]
      }
    ]
  },
  "transform_declarations": {
    "FINAL_OUTPUT": {
      "object": {
        "vital": {
          "object": {
            "record_id": {
              "xpath": "record_id"
            },
            "item_id": {
              "xpath": "item_id"
            },
            "captured_by": {
              "xpath": "vitals_captured_by"
            },
            "name": {
              "xpath": "vitals_name"
            },
            "standard_vital": {
              "xpath": "standard_vital"
            },
            "value": {
              "xpath": "vitals_value"
            },
            "modified_date": {
              "xpath": "modified_date"
            },
            "patient": {
              "object": {
                "id": {
                  "xpath": "patient_id"
                },
                "account_no": {
                  "xpath": "patient_account_no"
                }
              }
            },
            "encounter": {
              "object": {
                "id": {
                  "xpath": "encounter_id"
                }
              }
            },
            "provider": {
              "object": {
                "npi": {
                  "xpath": "provider_npi"
                }
              }
            },
            "practice": {
              "object": {
                "apuid": {
                  "xpath": "apuid"
                }
              }
            }
          }
        }
      }
    }
  }
}