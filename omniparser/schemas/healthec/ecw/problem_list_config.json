{
  "parser_settings": {
    "version": "omni.2.1",
    "file_format_type": "csv2"
  },
  "file_declaration": {
    "delimiter": "|",
    "replace_double_quotes": true,
    "records": [
      {
        "rows": 1,
        "min": 1,
        "max": 1
      },
      {
        "is_target": true,
        "columns": [
          {
            "name": "apuid",
            "index": 1
          },
          {
            "name": "record_id",
            "index": 2
          },
          {
            "name": "patient_id",
            "index": 3
          },
          {
            "name": "patient_account_no",
            "index": 4
          },
          {
            "name": "asmt_id",
            "index": 5
          },
          {
            "name": "onset_date",
            "index": 6
          },
          {
            "name": "code",
            "index": 7
          },
          {
            "name": "name",
            "index": 8
          },
          {
            "name": "encounter_id",
            "index": 9
          },
          {
            "name": "status",
            "index": 10
          },
          {
            "name": "pt_condition",
            "index": 11
          },
          {
            "name": "risk",
            "index": 12
          },
          {
            "name": "added_date",
            "index": 13
          },
          {
            "name": "resolved_on",
            "index": 14
          }
        ]
      }
    ]
  },
  "transform_declarations": {
    "FINAL_OUTPUT": {
      "object": {
        "problem_list": {
          "object": {
            "record_id": {
              "xpath": "record_id"
            },
            "asmt_id": {
              "xpath": "asmt_id"
            },
            "onset_date": {
              "xpath": "onset_date"
            },
            "code": {
              "xpath": "code"
            },
            "name": {
              "xpath": "name"
            },
            "status": {
              "xpath": "status"
            },
            "pt_condition": {
              "xpath": "pt_condition"
            },
            "risk": {
              "xpath": "risk"
            },
            "added_date": {
              "xpath": "added_date"
            },
            "resolved_on": {
              "xpath": "resolved_on"
            },
            "patient": {
              "object": {
                "id": {
                  "xpath": "patient_id"
                },
                "account_no": {
                  "xpath": "patient_account_no"
                }
              }
            },
            "encounter": {
              "object": {
                "id": {
                  "xpath": "encounter_id"
                }
              }
            },
            "practice": {
              "object": {
                "apuid": {
                  "xpath": "apuid"
                }
              }
            }
          }
        }
      }
    }
  }
}