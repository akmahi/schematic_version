          },
          {
            "name": "quantity",
            "index": 17
          },
          {
            "name": "refills",
            "index": 18
          },
          {
            "name": "strength",
            "index": 19
          },
          {
            "name": "formulation",
            "index": 20
          },
          {
            "name": "route",
            "index": 21
          },
          {
            "name": "frequency",
            "index": 22
          },
          {
            "name": "take",
            "index": 23
          },
          {
            "name": "med_status",
            "index": 24
          }
        ]
      }
    ]
