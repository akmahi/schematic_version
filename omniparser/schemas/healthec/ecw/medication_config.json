              }
            },
            "encounter": {
              "object": {
                "id": {
                  "xpath": "encounter_id"
                }
              }
            },
            "provider": {
              "object": {
                "id": {
                  "xpath": "provider_id"
                },
                "npi": {
                  "xpath": "provider_npi"
                }
              }
            },
            "practice": {
              "object": {
                "apuid": {
                  "xpath": "apuid"
                }
              }
            }
          }
        }
      }
    }
  }
}