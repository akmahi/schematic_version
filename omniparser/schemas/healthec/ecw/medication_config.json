            },
            "code_system": {
              "xpath": "medication_code_system"
            },
            "code": {
              "xpath": "medication_code"
            },
            "description": {
              "xpath": "medication_description"
            },
            "prescription_date": {
              "xpath": "prescription_date"
            },
            "prescription_begin_date": {
              "xpath": "prescription_begin_date"
            },
            "prescription_end_date": {
              "xpath": "prescription_end_date"
            },
            "days_supply": {
              "xpath": "days_supply"
            },
            "quantity": {
              "xpath": "quantity"
            },
            "refills": {
              "xpath": "refills"
            },
            "strength": {
              "xpath": "strength"
            },
            "formulation": {
