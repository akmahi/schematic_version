          },
          {
            "name": "provider_id",
            "index": 7
          },
          {
            "name": "provider_npi",
            "index": 8
          },
          {
            "name": "medication_origin_id",
            "index": 9
          },
          {
            "name": "medication_code_system",
            "index": 10
