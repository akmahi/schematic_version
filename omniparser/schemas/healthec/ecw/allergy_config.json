{
  "parser_settings": {
    "version": "omni.2.1",
    "file_format_type": "csv2"
  },
  "file_declaration": {
    "delimiter": "|",
    "replace_double_quotes": true,
    "records": [
      {
        "rows": 1,
        "min": 1,
        "max": 1
      },
      {
        "is_target": true,
        "columns": [
          {
            "name": "apuid",
            "index": 1
          },
          {
            "name": "record_id",
            "index": 2
          },
          {
            "name": "patient_id",
            "index": 3
          },
          {
            "name": "patient_account_no",
            "index": 4
          },
          {
            "name": "encounter_id",
            "index": 5
          },
          {
            "name": "encounter_date",
            "index": 6
          },
          {
            "name": "povider_npi",
            "index": 7
          },
          {
            "name": "item_id",
            "index": 8
          },
          {
            "name": "allergies_verified",
            "index": 9
          },
          {
            "name": "allergen_type",
            "index": 10
          },
          {
            "name": "allergy_description",
            "index": 11
          },
          {
            "name": "allergy_reaction_text",
            "index": 12
          },
          {
            "name": "allergy_status",
            "index": 13
          },
          {
            "name": "allergy_id",
            "index": 14
          },
          {
            "name": "modified_date",
            "index": 15
          }
        ]
      }
    ]
  },
  "transform_declarations": {
    "FINAL_OUTPUT": {
      "object": {
        "allergy": {
          "object": {
            "record_id": {
              "xpath": "record_id"
            },
            "item_id": {
              "xpath": "item_id"
            },
            "allergies_verified": {
              "xpath": "allergies_verified"
            },
            "allergen_type": {
              "xpath": "allergen_type"
            },
            "description": {
              "xpath": "allergy_description"
            },
            "reaction_text": {
              "xpath": "allergy_reaction_text"
            },
            "status": {
              "xpath": "allergy_status"
            },
            "id": {
              "xpath": "allergy_id"
            },
            "modified_date": {
              "xpath": "modified_date"
            },
            "patient": {
              "object": {
                "id": {
                  "xpath": "patient_id"
                },
                "account_no": {
                  "xpath": "patient_account_no"
                }
              }
            },
            "provider": {
              "object": {
                "npi": {
                  "xpath": "provider_npi"
                }
              }
            },
            "encounter": {
              "object": {
                "id": {
                  "xpath": "encounter_id"
                },
                "date": {
                  "xpath": "encounter_date"
                }
              }
            },
            "practice": {
              "object": {
                "apuid": {
                  "xpath": "apuid"
                }
              }
            }
          }
        }
      }
    }
  }
}