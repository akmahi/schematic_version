{
  "parser_settings": {
    "version": "omni.2.1",
    "file_format_type": "csv2"
  },
  "file_declaration": {
    "delimiter": "|",
    "records": [
      {
        "rows": 1,
        "min": 1,
        "max": 1
      },
      {
        "is_target": true,
        "columns": [
          {
            "name": "apuid",
            "index": 1
          },
          {
            "name": "record_id",
            "index": 2
          },
          {
            "name": "user_id",
            "index": 3
          },
          {
            "name": "user_type",
            "index": 4
          },
          {
            "name": "ufname",
            "index": 5
          },
          {
            "name": "ulname",
            "index": 6
          },
          {
            "name": "provider_npi",
            "index": 7
          },
          {
            "name": "speciality",
            "index": 8
          },
          {
            "name": "facility_id",
            "index": 9
          },
          {
            "name": "date_of_birth",
            "index": 10
          },
          {
            "name": "gender",
            "index": 11
          },
          {
            "name": "user_status",
            "index": 12
          }
        ]
      }
    ]
  },
  "transform_declarations": {
    "FINAL_OUTPUT": {
      "object": {
        "practice_user": {
          "object": {
            "record_id": {
              "xpath": "record_id"
            },
            "id": {
              "xpath": "user_id"
            },
            "last_name": {
              "xpath": "ulname"
            },
            "first_name": {
              "xpath": "ufname"
            },
            "type": {
              "xpath": "user_type"
            },
            "speciality": {
              "xpath": "speciality"
            },
            "date_of_birth": {
              "xpath": "date_of_birth"
            },
            "gender": {
              "xpath": "gender"
            },
            "status": {
              "xpath": "user_status"
            },
            "patient": {
              "object": {
                "id": {
                  "xpath": "patient_id"
                },
                "account_no": {
                  "xpath": "patient_account_no"
                }
              }
            },
            "facility": {
              "object": {
                "id": {
                  "xpath": "facility_id"
                }
              }
            },
            "practice": {
              "object": {
                "apuid": {
                  "xpath": "apuid"
                }
              }
            }
          }
        }
      }
    }
  }
}