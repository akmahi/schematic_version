{
  "parser_settings": {
    "version": "omni.2.1",
    "file_format_type": "csv2"
  },
  "file_declaration": {
    "delimiter": "|",
    "replace_double_quotes": true,
    "records": [
      {
        "columns": [
          {
            "name": "name",
            "index": 1
          },
          {
            "name": "billing_name",
            "index": 2
          },
          {
            "name": "type",
            "index": 3
          },
          {
            "name": "location_rank",
            "index": 4
          },
          {
            "name": "taxonomy_code",
            "index": 5
          },
          {
            "name": "ssn",
            "index": 6
          },
          {
            "name": "tin",
            "index": 7
          },
          {
            "name": "group_npi",
            "index": 8
          },
          {
            "name": "external_id",
            "index": 9
          },
          {
            "name": "line_1",
            "index": 10
          },
          {
            "name": "line_2",
            "index": 11
          },
          {
            "name": "city",
            "index": 12
          },
          {
            "name": "state",
            "index": 13
          },
          {
            "name": "zip",
            "index": 14
          },
          {
            "name": "country",
            "index": 15
          },
          {
            "name": "phone_work",
            "index": 16
          },
          {
            "name": "email",
            "index": 17
          },
          {
            "name": "fax",
            "index": 18
          },
          {
            "name": "lab_id",
            "index": 19
          },
          {
            "name": "lab_name",
            "index": 20
          },
          {
            "name": "contact_title",
            "index": 21
          },
          {
            "name": "contact_first_name",
            "index": 22
          },
          {
            "name": "contact_last_name",
            "index": 23
          },
          {
            "name": "contact_middle_initial",
            "index": 24
          },
          {
            "name": "contact_phone",
            "index": 25
          },
          {
            "name": "contact_email",
            "index": 26
          }
        ]
      }
    ]
  },
  "transform_declarations": {
    "FINAL_OUTPUT": {
      "object": {
        "practice": {
          "object": {
            "name": { "xpath": "name" },
            "billing_name": { "xpath": "billing_name" },
            "type": { "xpath": "type" },
            "location_rank": { "xpath": "location_rank" },
            "taxonomy_code": { "xpath": "taxonomy_code" },
            "ssn": { "xpath": "ssn" },
            "tin": { "xpath": "tin" },
            "group_npi": { "xpath": "group_npi" },
            "external_id":{ "xpath":"external_id" },
            "address": {
                "object": {
                    "line1": { "xpath": "line_1" },
                    "line2": { "xpath": "line_2" },
                    "city": { "xpath": "city" },
                    "state": { "xpath": "state" },
                    "zip": { "xpath": "zip" },
                    "country": { "xpath": "country" }
                }
            },
            "phone_work": { "xpath": "phone_work" },
            "email": { "xpath": "email" },
            "fax": { "xpath": "fax" },
            "lab": {
              "object": {
                "id": { "xpath": "lab_id" },
                "name": { "xpath": "lab_name" }
              }
            },
            "contact": {
              "object": {
                "title": { "xpath": "contact_title" },
                "first_name": { "xpath": "contact_first_name" },
                "last_name": { "xpath": "contact_last_name" },
                "middle_initial": { "xpath": "contact_middle_initial" },
                "phone": { "xpath": "contact_phone" },
                "email": { "xpath": "contact_email" }
              }
            }
          }
        }
      }
    }
  }
}